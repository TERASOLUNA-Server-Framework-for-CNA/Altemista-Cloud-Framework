<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
		
	<!--
		Unauthenticated client for org.altemista.cloudfwk.it.soap.secured.BottomUpSecuredExampleService
	 -->
	
	<bean id="bottomUpSecuredExampleServiceClient" class="org.altemista.cloudfwk.core.soap.SimpleJaxWsPortProxyFactoryBean">
		<property name="serviceInterface" value="org.altemista.cloudfwk.it.soap.secured.BottomUpSecuredExampleService" />
		<property name="endpointAddress" value="#{systemProperties['integration-test.url']}/soap/BottomUpSecuredExampleService" />
	</bean>

</beans>