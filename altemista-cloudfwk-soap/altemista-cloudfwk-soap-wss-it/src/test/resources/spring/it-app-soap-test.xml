<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
		
	<!--
		Client for org.altemista.cloudfwk.it.soap.BottomUpExampleService
	 -->
	 
	<bean id="bottomUpExampleServiceClient" class="org.altemista.cloudfwk.core.soap.SimpleJaxWsPortProxyFactoryBean">
		<property name="serviceInterface" value="org.altemista.cloudfwk.it.soap.BottomUpExampleService" />
		<property name="endpointAddress" value="#{systemProperties['integration-test.url']}/soap/BottomUpExampleService" />
	</bean>

</beans>