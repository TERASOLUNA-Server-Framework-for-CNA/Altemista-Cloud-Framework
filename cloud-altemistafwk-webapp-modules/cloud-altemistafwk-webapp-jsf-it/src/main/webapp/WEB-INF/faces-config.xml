<?xml version='1.0' encoding='UTF-8'?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>submenu1</from-outcome>
			<to-view-id>/option1.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>submenu2</from-outcome>
			<to-view-id>/option2.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>submenu3</from-outcome>
			<to-view-id>/does-not-exist.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

</faces-config>
